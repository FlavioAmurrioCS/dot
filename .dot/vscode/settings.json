{
  "[jsonc]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "editor.detectIndentation": false,
  "editor.fontFamily": "'Cascadia Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.formatOnType": false,
  "editor.insertSpaces": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    100,
    120
  ],
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "jsp": "html"
  },
  "explorer.openEditors.visible": 10,
  "explorer.sortOrder": "type",
  "files.associations": {
    "*rc": "shellscript",
    ".jsbeautifyrc": "jsonc",
    "ssh_config_*": "ssh_config"
  },
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.venv/**": true,
    "**/node_modules/**": true,
    "**/build/**": true,
    "**/examples/**": true,
    "**/output/**": true
  },
  "git.autoRepositoryDetection": "openEditors",
  "git.autoStash": true,
  "git.autofetch": true,
  "git.defaultCloneDirectory": "${env:HOME}/projects",
  "git.detectSubmodules": false,
  "git.enableSmartCommit": true,
  "git.ignoreLegacyWarning": true,
  "git.smartCommitChanges": "tracked",
  "gitlens.blame.ignoreWhitespace": true,
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "java.debug.settings.forceBuildBeforeLaunch": false,
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.saveActions.organizeImports": true,
  "leetcode.defaultLanguage": "python3",
  "leetcode.hint.commandShortcut": false,
  "leetcode.hint.commentDescription": false,
  "leetcode.hint.configWebviewMarkdown": false,
  "leetcode.showDescription": "Both",
  "leetcode.workspaceFolder": "/Users/flavio/projects/leetcode",
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.useBrowserPreview": true,
  "projectManager.any.baseFolders": [
    "$home/projects"
  ],
  "projectManager.any.maxDepthRecursion": 1,
  "pyright.useLibraryCodeForTypes": true,
  "python.insidersChannel": "off",
  "python.jediEnabled": false,
  "python.pythonPath": "/usr/bin/python3",
  "search.exclude": {
    "**/.venv": true
  },
  "shellcheck.enableQuickFix": true,
  "shellcheck.exclude": [
    "SC2088"
  ],
  "terminal.integrated.shell.osx": "/bin/bash",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.zoomLevel": 0,
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "terminal.integrated.rendererType": "dom",
  "shellcheck.customArgs": [
    "-x"
  ],
  "remote.extensionKind": {
    "ms-azuretools.vscode-docker": "workspace"
  },
  "window.openFoldersInNewWindow": "on",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorTheme": "Deepdark Material Theme | Full Black Version",
  "vsonline.accountProvider": "Microsoft",
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.message.update.show": false,
  "cSpell.userWords": [
    "CVPR",
    "Kaggle",
    "LSTM",
    "dataset",
    "egpaper",
    "kang",
    "pyoung",
    "regressional"
  ],
  "grammarly.userWords": [
    "Pyoung",
    "csv"
  ],
  "cmake.configureOnOpen": true,
  "C_Cpp.updateChannel": "Insiders",
  "[javascript]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "python.languageServer": "Pylance",
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, ColumnLimit: 100 }",
  "codespaces.accountProvider": "Microsoft",
  "todo-tree.tree.showScanModeButton": false,
  "python.showStartPage": true,
  "python.formatting.autopep8Args": [
    "--max-line-length",
    "100",
    "--aggressive",
    "--aggressive"
  ],
  "glsl-linter.validatorPath": "/Users/flavio/.local/bin/glslangValidator",
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "leetcode.filePath": {


    "default": {
      "folder": "/Users/flavio/projects/leetcode",
      "filename": "${id}.${kebab-case-name}.${ext}"
    }
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "rewrap.wrappingColumn": 100,
  "files.autoSave": "afterDelay",


}
