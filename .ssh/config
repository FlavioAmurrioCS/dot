IgnoreUnknown UseKeychain,AddKeysToAgent

Host NOMUX
  ControlPath none

Host ec2
  User bitnami
  HostName ec2-3-95-32-5.compute-1.amazonaws.com
  IdentityFile ~/.ssh/swe645.pem

Host *
  Compression yes
  ForwardAgent yes
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials yes
  KeepAlive yes
  ServerAliveCountMax 6
  ServerAliveInterval 15
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  LogLevel FATAL
  ControlPath /tmp/%r@%h:%p
  ControlMaster auto
  ControlPersist yes
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_ed25519

Host pihole
  User pi
  HostName 192.168.86.69

